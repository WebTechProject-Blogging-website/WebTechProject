<% var header = '../header'; %>
<%- include(header) %>
        
<div class="container-fluid mt-3">
    <div class="jumbotron">
            <header>
                    <div class="page-header">
                        <div class="row mt-6">
                            <div class="col-sm-6">
                            <h1>Edit User:</h1>
                            </div>
                        </div>
                    </div>
            </header>
    <form action="/users/edit/<%= id %>" method="POST" id="edit">
    <div class="row mt-2">
        <div class="col-sm-6">
            <h4>Username: </h4>
        </div>
        <div class="col-sm-6">
            <p><%- username["username"] %></p>
        </div>
    </div>
    <div class="row mt-2">
        <div class="col-sm-6">
            <h4>Password: </h4>
        </div>
        <div class="col-sm-6">
            <p>><%- username["password"] %></p>
        </div>
    </div>
    <div class="row mt-2">
        <div class="col-sm-6">
            <h4>Access: </h4>
        </div>
        <div class="col-sm-6">
            <p><%- username["access"] %></p>
        </div>
    </div>
    </form>
    </div>
</div>

<% var footer = '../footer'; %>
<%- include(footer) %>